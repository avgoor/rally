{% set concurrency = 4000 %}
{% set times = 40000 %}
{% set users = 5 %}
{% set tenants = 100 %}
{
  "NeutronNetworks.create_and_delete_networks": [
  {
    "runner": {
      "type": "constant",
      "concurrency": {{concurrency}},
      "times": {{times}}
    },
    "context": {
      "users": {
        "tenants": {{users}},
        "users_per_tenant": {{tenants}}
      },
      "quotas": {
        "neutron": { "network": -1 }
      }
    },
    "sla": { "failure_rate": { "max": 0 } }
  }
  ]
}
